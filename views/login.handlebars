<html>
<head>
	<title>Log In</title>
    <meta charset="utf-8">
	<link href="/css/bootstrap-grid.css" rel="stylesheet">
    <link href="/css/bootstrap.css" rel="stylesheet">
    <link href="/css/login.css" rel="stylesheet">
    <script src="https://code.jquery.com/jquery.js"></script>
	<script src="/js/jquery-1.11.0.js"></script>
	<script src="/js/bootstrap.bundle.js"></script>
    <!-- script src="/js/login.js"></script> -->
</head>

<body>
    <!-- Navbar -->
    <nav class="navbar fixed-top navbar-expand-lg">
        <ul class="navbar-nav mr-auto">
            <li class="nav-item" id="nav-item-logo">
                <a href="/"><img src="images/logo.png" alt="logo" id="home-logo"></a>
            </li>
            <li class="nav-item">
                <a href="/"><h5>photosign</h5></a>
            </li>
        </ul>
    </nav>

    <!-- Login -->
    <div class="container-fluid h-100 w-100 d-flex justify-content-center align-items-center" id="about">
        <div class="grid w-100">
            <div class="row">
                <div class="col col-lg-12 text-center">
                    <h2 class="custom-h2-signup">Log In</h2>
                </div>
            </div>
            <div class="row">
                <div class="col col-lg-4"></div>
                <div class="col col-lg-4 text-center">
                    <form>
                        <div class="form-group text-center pad-custom">
                            <input type="email" class="form-control email-custom" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
                        </div>
                    </form>
                </div>
                <div class="col col-lg-4"></div>
            </div>
            <div class="row">
                <div class="col col-lg-4"></div>
                <div class="col col-lg-4 text-center">
                    <form>
                        <div class="form-group text-center pad-custom">
                            <input type="password" class="form-control email-custom" id="exampleInputPassword1" placeholder="Password">
                        </div>
                    </form>
                </div>
                <div class="col col-lg-4"></div>
            </div>
            <div class="row">
                <div class="col col-lg-4"></div>
                <div class="col col-lg-4 text-center">
                    <button type="button" class="btn btn-custom-large" href="/home">Sign In</button>
                </div>
                <div class="col col-lg-4"></div>
            </div>
        </div>
    </div>
</body>

</html>